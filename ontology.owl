<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xml:base="http://example.org/onto.owl"
         xmlns="http://example.org/onto.owl#">

<owl:Ontology rdf:about="http://example.org/onto.owl"/>

<owl:DatatypeProperty rdf:about="#RiskLevel">
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
  <rdfs:domain rdf:resource="#KnownObject"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#RecommendedAction">
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  <rdfs:domain rdf:resource="#KnownObject"/>
</owl:DatatypeProperty>

<owl:Class rdf:about="#KnownObject">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
</owl:Class>

<owl:Class rdf:about="#SuspiciousObject">
  <rdfs:subClassOf rdf:resource="#KnownObject"/>
</owl:Class>

<owl:Class rdf:about="#SafeObject">
  <rdfs:subClassOf rdf:resource="#KnownObject"/>
</owl:Class>

<owl:NamedIndividual rdf:about="#car">
  <rdf:type rdf:resource="#SafeObject"/>
  <RiskLevel rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">0.1</RiskLevel>
  <RecommendedAction rdf:datatype="http://www.w3.org/2001/XMLSchema#string">ignore</RecommendedAction>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#dog">
  <rdf:type rdf:resource="#SafeObject"/>
  <RiskLevel rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">0.2</RiskLevel>
  <RecommendedAction rdf:datatype="http://www.w3.org/2001/XMLSchema#string">ignore</RecommendedAction>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#person">
  <rdf:type rdf:resource="#SuspiciousObject"/>
  <RiskLevel rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">0.8</RiskLevel>
  <RecommendedAction rdf:datatype="http://www.w3.org/2001/XMLSchema#string">investigate</RecommendedAction>
</owl:NamedIndividual>


</rdf:RDF>
